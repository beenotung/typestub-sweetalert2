  },
  "devDependencies": {
    "@angular/compiler": "^4.4.3",
    "@angular/compiler-cli": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@angular/tsc-wrapped": "^4.4.3",
    "@ionic/app-scripts": "^2.1.4",
    "codelyzer": "^3.2.0",
    "npm-run-all": "^4.1.1",
    "rxjs": "^5.4.3",
